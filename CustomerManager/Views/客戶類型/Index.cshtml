@model IEnumerable<CustomerManager.Models.客戶類型>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.客戶分類)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.客戶分類)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.客戶分類 }) |
                @Html.ActionLink("Details", "Details", new { id = item.客戶分類 }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.客戶分類 })
            </td>
        </tr>
    }
</table>